<h1>Assignments#edit</h1>
<p>Find me in app/views/assignments/edit.html.erb</p>

<%= form_for @assignment do |f| %>
  <div>
    <%= f.label :name %>
    <%= f.text_field :name %>
  </div>
  <div>
    <%= f.label :description %>
    <%= f.text_area :description %>
  </div>
  <%= f.submit %>
<% end %>